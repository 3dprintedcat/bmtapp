{
	"name": "bmt_services_center",
	"version": "0.1.0",
	"main": "src/electron.js",
	"private": true,
	"homepage": "https://bmtsc.org/",
	"description": "Black Magic Trading Services Center connecting Star citizen orgs (made by the community).",
	"author": "3D Printed Cat <8skittles5@gmail.com>",
	"build": {
		"asar": true,
		"appId": "org.bmtsc.bmtapp",
		"directories": {
			"output": "build"
		},
		"files": [
			"build/**/*",
			"node_modules/**/*",
			"src/electron.js"
		],
		"icon": "public/logo",
		"mac": {
			"category": "public.app-category.developer-tools"
		},
		"win": {
			"target": "nsis"
		}
	},
	 "dependencies": {
    "@capacitor/android": "5.0.4",
    "@capacitor/app": "5.0.2",
    "@capacitor/core": "^5.0.4",
    "@capacitor/haptics": "5.0.2",
    "@capacitor/ios": "5.0.4",
    "@capacitor/keyboard": "5.0.2",
    "@capacitor/status-bar": "5.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.2.2",
    "axios": "^1.3.4",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "custom-electron-titlebar": "^4.2.3",
    "electron-titlebar": "^0.0.3",
    "otp-io": "^1.1.1",
    "otpauth": "^9.0.2",
    "otplib": "^12.0.1",
    "path-browserify": "^1.0.1",
    "qrcode": "^1.5.1",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "speakeasy": "^2.0.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "web-vitals": "^2.1.4"
  },
	"scripts": {
		"react-start": "react-app-rewired start",
		"electron-start": "electron .",
		"start": "concurrently \"cross-env npm run react-start\" \"wait-on http://localhost:3000 && npm run electron-start\"",
		"build": "react-app-rewired build",
		"test": "react-app-rewired test",
		"eject": "react-app-rewired eject",
		"ionic:build": "npm run build",
		"ionic:serve": "npm run start"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"concurrently": "^8.0.1",
		"cross-env": "^7.0.3",
		"electron": "^23.3.4",
		"electron-builder": "^23.6.0",
		"wait-on": "^7.0.1"
	}
}
